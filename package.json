{
  "name": "private-console",
  "version": "0.1.0",
  "bin": {
    "private-console": "bin/private-console.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "private-console": "tsc && cdk --app 'npx ts-node bin/private-console.ts'",
    "private-console-deploy": "tsc && cdk --app 'npx ts-node bin/private-console.ts' deploy --require-approval never"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "18.11.18",
    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "aws-cdk": "2.64.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.64.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
